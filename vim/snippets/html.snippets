snippet script
	<script src="${1}.js" type="text/javascript"></script>${2}
snippet #
	<div id="${1:id}">${2}</div>
snippet .
	<div class="${1:class}">${2}</div>
snippet fieldset
	<fieldset>
		<legend>${1:name}</legend>
		${2}
	</fieldset>
snippet h1
	<h1>${1}</h1>${2}
snippet h2
	<h2>${1}</h2>${2}
snippet h3
	<h3>${1}</h2>${2}
snippet p
	<p>${1}</p>${2}
snippet link
	<link rel="${1:stylesheet}" type="text/css" media="${2:screen}" href="${3:screen.css}" />${4}
